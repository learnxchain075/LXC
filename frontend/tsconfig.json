{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    // "types": ["react", "react-dom", "react-select", "react-router-dom"]
    "types": ["vite/client", "vite-plugin-svgr/client","node", "react", "react-dom", "react-select", "react-router-dom"],
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}